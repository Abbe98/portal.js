{
  "name": "@europeana/portal",
  "version": "0.0.1",
  "description": "Europeana Portal",
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>"
  ],
  "private": true,
  "keywords": [
    "code4lib",
    "incubator"
  ],
  "license": "EUPL-1.1",
  "engines": {
    "node": "^10",
    "npm": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europeana/incubator-portal-vue-nuxt.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "rm -rf node_modules && npm install --only=production && npm run build && rm portaljs.zip && zip -r portaljs.zip ./ -x *.git* && ibmcloud fn action update portaljs portaljs.zip --kind nodejs:10 --web raw"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "elastic-apm-node": "^2.0.6",
    "express": "^4.16.3",
    "expressjs-openwhisk": "^1.1.0",
    "morgan": "^1.9.1",
    "nuxt": "^2.0.0",
    "vcap_services": "^0.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "nodemon": "^1.11.0"
  },
  "main": "server/less.js"
}
