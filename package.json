{
  "name": "@europeana/portal",
  "version": "0.5.0",
  "description": "Europeana Portal",
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>",
    "Lutz Biedinger <lutz.biedinger@europeana.eu>",
    "Mirjam Verloop <mirjam.verloop@europeana.eu>"
  ],
  "private": true,
  "keywords": [
    "code4lib",
    "incubator"
  ],
  "license": "EUPL-1.2",
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europeana/incubator-portal-vue-nuxt.git"
  },
  "scripts": {
    "apicache:start": "cservice --cli false tests/apicache/index.js",
    "build-storybook": "build-storybook -s ./.storybook/assets",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "generate": "nuxt generate",
    "lint": "npm run stylelint && eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "size": "npm run build && size-limit",
    "start:test": "cross-env NODE_ENV=test PORT=1337 node server/index.js",
    "start:test:caching": "cross-env NODE_ENV=test CTF_INSECURE=1 CTF_CDA_BASE_PATH=/contentful CTF_CDA_HOST=localhost.localdomain:4000 EUROPEANA_RECORD_API_ORIGIN=http://localhost:4000 EUROPEANA_ENTITY_API_ORIGIN=http://localhost:4000 EUROPEANA_NEWSPAPER_API_ORIGIN=http://localhost:4000/newspaper EUROPEANA_THUMBNAIL_API_ORIGIN=http://localhost:4000 PORT=1337 node server/index.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "stylelint": "stylelint \"**/*.scss\" \"**/*.vue\" --config .stylelintrc.js",
    "test:chrome:headless": "cross-env browser=chromeHeadless cucumber-js",
    "test:chrome": "cross-env browser=chrome cucumber-js",
    "test:ci": "npm run test:unit && npm run build:test && npx size-limit && npm run test:e2e:ci",
    "test:e2e:ci": "concurrently 'npm run apicache:start' 'npm run start:test:caching' 'npm run test:chrome:headless -- -p all' --success first --kill-others-on-fail",
    "test:e2e": "concurrently 'npm run apicache:start' 'npm run start:test:caching' 'npm run test:chrome -- -p all' --success first --kill-others",
    "test:gecko": "cross-env browser=gecko cucumber-js --no-strict",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup",
    "test": "npm run test:unit && npm run build:test && npx size-limit && npm run test:e2e"
  },
  "dependencies": {
    "@elastic/apm-rum": "^4.7.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-tag-manager": "^2.2.0",
    "autosuggest-highlight": "^3.1.1",
    "bootstrap-vue": "^2.0.0",
    "contentful": "^7.14.0",
    "cookie-universal-nuxt": "^2.0.18",
    "cross-env": "^5.2.1",
    "deep-object-diff": "^1.1.0",
    "deepmerge": "^4.2.2",
    "elastic-apm-node": "^2.16.1",
    "express": "^4.16.4",
    "express-ipfilter": "^1.0.1",
    "http-auth": "^3.2.3",
    "is-https": "^1.0.0",
    "lodash": "^4.17.15",
    "marked": "^0.7.0",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "nuxt": "^2.11.0",
    "nuxt-i18n": "^6.3.0",
    "qs": "^6.6.0",
    "redirect-ssl": "^1.4.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/generator": "^7.6.0",
    "@babel/parser": "^7.3.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@babel/types": "^7.6.1",
    "@size-limit/preset-app": "^2.1.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/vue": "^5.0.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "apicache": "^1.5.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "chromedriver": "^80.0.0",
    "cluster-service": "^2.1.3",
    "concurrently": "^4.1.0",
    "contentful-cli": "^1.2.3",
    "cors": "^2.8.5",
    "cssesc": "^3.0.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "geckodriver": "^1.19.1",
    "http-proxy-middleware": "^0.20.0",
    "is-reachable": "^3.0.0",
    "nightwatch": "^1.3.0",
    "nightwatch-accessibility": "^1.8.0",
    "nightwatch-api": "^3.0.0",
    "nock": "^10.0.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "redis": "^3.0.2",
    "regenerator-runtime": "0.13.3",
    "sass-loader": "^8.0.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "storybook-vue-router": "^1.0.6",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.1.0",
    "svg-url-loader": "^3.0.2",
    "vue-i18n": "^8.14.1",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.11"
  }
}
